@using FarmTrack.Models.Entities;
@{

    ViewData["Title"] = "Dashboard";
    var user = Context.Session.GetObject<User>("CurrentUser") ?? new User();
    ViewData["SessionUsername"] = user.Username;

}
<h1>Welcome, @ViewData["SessionUsername"]</h1>

<h3>Temperature at noon tomorrow: @ViewData["TemperatureAtNoon"]</h3>
